{
  "name": "js-algorithms",
  "private": true,
  "version": "1.5.6",
  "type": "module",
  "keywords": [
    "algorithms",
    "javascript"
  ],
  "description": "Interactive JavaScript Algorithms",
  "repository": "github:eldarlrd/js-algorithms",
  "homepage": "https://eldarlrd.is-a.dev/js-algorithms",
  "author": "Eldar Pashazade <eldarlrd@pm.me> (https://eldarlrd.is-a.dev)",
  "license": "AGPL-3.0-only",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && cp 404.html dist",
    "preview": "vite preview",
    "deployer": "pnpm build && gh-pages -b prod -m 'build(prod): deployment' -d dist",
    "lint": "eslint src",
    "pretty": "prettier '{src,__{tests,mocks,snapshots}__}/**/*.{ts,tsx}' --write",
    "format": "pnpm lint --fix && pnpm pretty",
    "cut": "knip --fix --cache"
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSameLine": true,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "experimentalTernaries": true
  },
  "dependencies": {
    "@chakra-ui/react": "catalog:frontend",
    "@emotion/react": "catalog:frontend",
    "@emotion/styled": "catalog:frontend",
    "@fontsource/ubuntu": "catalog:frontend",
    "@fontsource/ubuntu-mono": "catalog:frontend",
    "@fortawesome/fontawesome-svg-core": "catalog:frontend",
    "@fortawesome/free-brands-svg-icons": "catalog:frontend",
    "@fortawesome/free-solid-svg-icons": "catalog:frontend",
    "@fortawesome/react-fontawesome": "catalog:frontend",
    "framer-motion": "catalog:frontend",
    "preact": "catalog:frontend",
    "react": "catalog:frontend",
    "react-dom": "catalog:frontend",
    "react-router": "catalog:frontend",
    "react-syntax-highlighter": "catalog:frontend"
  },
  "devDependencies": {
    "@babel/core": "catalog:build",
    "@eslint/js": "catalog:lint",
    "@preact/preset-vite": "catalog:build",
    "@types/node": "catalog:types",
    "@types/react": "catalog:types",
    "@types/react-dom": "catalog:types",
    "@types/react-syntax-highlighter": "catalog:types",
    "eslint": "catalog:lint",
    "eslint-config-prettier": "catalog:lint",
    "eslint-import-resolver-typescript": "catalog:lint",
    "eslint-plugin-import": "catalog:lint",
    "eslint-plugin-jsx-a11y": "catalog:lint",
    "eslint-plugin-n": "catalog:lint",
    "eslint-plugin-no-relative-import-paths": "catalog:lint",
    "eslint-plugin-promise": "catalog:lint",
    "eslint-plugin-react": "catalog:lint",
    "eslint-plugin-react-hooks": "catalog:lint",
    "eslint-plugin-react-refresh": "catalog:lint",
    "gh-pages": "catalog:script",
    "globals": "catalog:lint",
    "jiti": "catalog:lint",
    "knip": "catalog:lint",
    "prettier": "catalog:lint",
    "typescript": "catalog:build",
    "typescript-eslint": "catalog:lint",
    "vite": "catalog:build"
  }
}
