{
  "name": "js-algorithms",
  "private": true,
  "version": "1.3.0",
  "type": "module",
  "keywords": [
    "algorithms",
    "javascript"
  ],
  "description": "Interactive JavaScript Algorithms",
  "repository": "github:eldarlrd/js-algorithms",
  "homepage": "https://eldarlrd.github.io/js-algorithms",
  "author": "Eldar Pashazade <eldarlrd@pm.me> (https://eldarlrd.github.io)",
  "license": "AGPL-3.0-only",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deployer": "pnpm build && gh-pages -b prod -m 'build(prod): deployment' -d dist",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives",
    "pretty": "prettier '{src,__{tests,mocks,snapshots}__}/**/*.{ts,tsx}' --write",
    "format": "pnpm lint --fix && pnpm pretty"
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSameLine": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/ubuntu": "^5.0.8",
    "@fontsource/ubuntu-mono": "^5.0.18",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "framer-motion": "^11.0.5",
    "preact": "^10.19.5",
    "react": "npm:@preact/compat@^17.1.2",
    "react-dom": "npm:@preact/compat@^17.1.2",
    "react-intersection-observer": "^9.8.0",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@preact/preset-vite": "^2.8.1",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-promise": "^6.1.1",
    "gh-pages": "^6.1.1",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.3"
  }
}
