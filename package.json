{
  "name": "js-algorithms",
  "private": true,
  "version": "1.4.5",
  "type": "module",
  "keywords": [
    "algorithms",
    "javascript"
  ],
  "description": "Interactive JavaScript Algorithms",
  "repository": "github:eldarlrd/js-algorithms",
  "homepage": "https://eldarlrd.github.io/js-algorithms",
  "author": "Eldar Pashazade <eldarlrd@pm.me> (https://eldarlrd.github.io)",
  "license": "AGPL-3.0-only",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deployer": "pnpm build && gh-pages -b prod -m 'build(prod): deployment' -d dist",
    "lint": "eslint src --flag unstable_ts_config",
    "pretty": "prettier '{src,__{tests,mocks,snapshots}__}/**/*.{ts,tsx}' --write",
    "format": "pnpm lint --fix && pnpm pretty"
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSameLine": true,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "experimentalTernaries": true
  },
  "dependencies": {
    "@chakra-ui/react": "^2.10.2",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/ubuntu": "^5.1.0",
    "@fontsource/ubuntu-mono": "^5.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "framer-motion": "^11.11.7",
    "preact": "^10.24.2",
    "react": "npm:@preact/compat@^18.3.1",
    "react-dom": "npm:@preact/compat@^18.3.1",
    "react-intersection-observer": "^9.13.1",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/plugin-transform-react-jsx": "^7.25.7",
    "@eslint/js": "^9.12.0",
    "@preact/preset-vite": "^2.9.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-n": "^17.11.1",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "gh-pages": "^6.2.0",
    "globals": "^15.11.0",
    "jiti": "^2.3.3",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.4.8"
  }
}
